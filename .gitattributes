# SCM syntax highlighting & preventing 3-way merges
pixi.lock merge=binary linguist-language=YAML linguist-generated=true
flattened-codebase.xml filter=lfs diff=lfs merge=lfs -text
conda-packages/*.whl filter=lfs diff=lfs merge=lfs -text
conda-packages/*.conda filter=lfs diff=lfs merge=lfs -text
local-channel/linux-64/*.whl filter=lfs diff=lfs merge=lfs -text
local-channel/linux-64/*.conda filter=lfs diff=lfs merge=lfs -text
